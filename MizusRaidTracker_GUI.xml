<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
 http://wowprogramming.com/FrameXML/UI.xsd">
  <Script file="MizusRaidTracker_GUI.lua"/>
  <EditBox name="MRT_GUI_Editbox_Template" virtual="true" historyLines="0" enableKeyboard="true" letters="200">
    <Size x="250" y="32"/>
    <Layers>
      <Layer level="BACKGROUND">
        <Texture file="Interface\ChatFrame\UI-ChatInputBorder-Left">
          <Size>
            <AbsDimension x="140" y="32"></AbsDimension>
          </Size>
          <Anchors>
            <Anchor point="LEFT">
              <Offset>
                <AbsDimension x="-15" y="0"></AbsDimension>
              </Offset>
            </Anchor>
          </Anchors>
          <TexCoords left="0" right="0.55" top="0" bottom="1.0"></TexCoords>
        </Texture>
        <Texture file="Interface\ChatFrame\UI-ChatInputBorder-Right">
          <Size>
            <AbsDimension x="140" y="32"></AbsDimension>
          </Size>
          <Anchors>
            <Anchor point="RIGHT">
              <Offset>
                <AbsDimension x="15" y="0"></AbsDimension>
              </Offset>
            </Anchor>
          </Anchors>
          <TexCoords left="0.45" right="1.0" top="0" bottom="1.0"></TexCoords>
        </Texture>
      </Layer>
    </Layers>
    <FontString inherits="ChatFontNormal"/>
  </EditBox>
  <Button name="MRT_GUI_Button_Template" virtual="true" hidden="false" inherits="GameMenuButtonTemplate">
    <Size x="85" y="22"/>
  </Button>
  <Frame name="MRT_GUI_Dialog_Template" virtual="true" hidden="true" toplevel="true" enableMouse="true" movable="true" parent="UIParent">
    <Size x="366" y="150"/>
    <Anchors>
      <Anchor point="CENTER" relativePoint="CENTER" relativeTo="$parent"/>
    </Anchors>
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <TileSize val="32"/>
      <EdgeSize val="32"/>
      <BackgroundInsets left="11" right="12" top="12" bottom="11"/>
    </Backdrop>
    <Layers>
      <Layer level="ARTWORK">
        <Texture name="$parent_HeaderTexture" file="Interface\DialogFrame\UI-DialogBox-Header">
          <Size x="300" y="64"/>
          <Anchors>
            <Anchor point="TOP">
              <Offset x="0" y="12"/>
            </Anchor>
          </Anchors>
        </Texture>
      </Layer>
      <Layer level="OVERLAY">
        <FontString name="$parent_Title" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="TOP">
              <Offset x="0" y="-2"/>
            </Anchor>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <Button name="$parent_OKButton" inherits="MRT_GUI_Button_Template">
        <Anchors>
          <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parent">
            <Offset x="25" y="15"/>
          </Anchor>
        </Anchors>
      </Button>
      <Button name="$parent_CancelButton" inherits="MRT_GUI_Button_Template">
        <Anchors>
          <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parent_OKButton">
            <Offset x="55" y="0"/>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnClick>MRT_GUI_HideDialogs();</OnClick>
        </Scripts>
      </Button>
    </Frames>
  </Frame>
  <Frame name="MRT_GUI_OneRowDialog" inherits="MRT_GUI_Dialog_Template">
    <Size y="120"/>
    <Layers>
      <Layer level="OVERLAY">
        <FontString name="$parent_EB1_Text" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="LEFT" relativePoint="TOPLEFT" relativeTo="$parent">
              <Offset x="58" y="-32"/>
            </Anchor>            
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <EditBox name="$parent_EB1" inherits="MRT_GUI_Editbox_Template">
        <Anchors>
          <Anchor point="LEFT" relativePoint="TOPLEFT" relativeTo="$parent">
            <Offset x="58" y="-64"/>
          </Anchor>
        </Anchors>
      </EditBox>
    </Frames>
  </Frame>
  <Frame name="MRT_GUIFrame" toplevel="true" enableMouse="true" movable="true" parent="UIParent" hidden="true">
    <Size x="800" y="600"/>
    <Anchors>
      <Anchor point="CENTER" relativePoint="CENTER" relativeTo="$parent"/>
    </Anchors>
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <BackgroundInsets left="11" right="12" top="12" bottom="11"/>
      <TileSize val="32"/>
      <EdgeSize val="32"/>
    </Backdrop>
    <Layers>
      <Layer level="ARTWORK">
        <Texture name="$parent_HeaderTexture" file="Interface\DialogFrame\UI-DialogBox-Header">
          <Size x="300" y="64"/>
          <Anchors>
            <Anchor point="TOP" relativePoint="TOP" relativeTo="$parent">
              <Offset x="0" y="12"/>
            </Anchor>
          </Anchors>
        </Texture>
      </Layer>
      <Layer level="OVERLAY">
        <FontString name="$parent_Title" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="TOP" relativePoint="TOP" relativeTo="$parent">
              <Offset x="0" y="-2"/>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parent_RaidLogTitle" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="TOPLEFT" relativePoint="TOPLEFT" relativeTo="$parent">
              <Offset x="30" y="-30"/>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parent_RaidBosskillsTitle" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="LEFT" relativePoint="LEFT" relativeTo="$parent_RaidLogTitle">
              <Offset x="275" y="0"/>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parent_RaidAttendeesTitle" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="LEFT" relativePoint="LEFT" relativeTo="$parent_RaidBosskillsTitle">
              <Offset x="265" y="0"/>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parent_BossLootTitle" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parent_RaidBosskillsTitle">
              <Offset x="0" y="-275"/>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parent_BossAttendeesTitle" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="LEFT" relativePoint="LEFT" relativeTo="$parent_BossLootTitle">
              <Offset x="345" y="0"/>
            </Anchor>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <Button name="$parent_RaidLog_Delete_Button" inherits="MRT_GUI_Button_Template"></Button>
      <Button name="$parent_RaidBosskills_Add_Button" inherits="MRT_GUI_Button_Template"></Button>
      <Button name="$parent_RaidBosskills_Delete_Button" inherits="MRT_GUI_Button_Template"></Button>
      <Button name="$parent_RaidAttendees_Add_Button" inherits="MRT_GUI_Button_Template"></Button>
      <Button name="$parent_RaidAttendees_Delete_Button" inherits="MRT_GUI_Button_Template"></Button>
      <Button name="$parent_BossLoot_Add_Button" inherits="MRT_GUI_Button_Template"></Button>
      <Button name="$parent_BossLoot_Delete_Button" inherits="MRT_GUI_Button_Template"></Button>
      <Button name="$parent_BossAttendees_Add_Button" inherits="MRT_GUI_Button_Template"></Button>   
    </Frames>
    <Scripts>
      <OnLoad>self:RegisterForDrag("LeftButton");</OnLoad>
      <OnDragStart>self:StartMoving();</OnDragStart>
      <OnDragStop>self:StopMovingOrSizing();</OnDragStop>
    </Scripts>
  </Frame>
</Ui>