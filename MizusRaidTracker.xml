<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
 http://wowprogramming.com/FrameXML/UI.xsd">
  <Script file="MizusRaidTracker.lua"/>
  <Frame name="MRT_MainFrame">
    <Scripts>
      <OnLoad>MRT_MainFrame_OnLoad(self);</OnLoad>
      <OnEvent>MRT_OnEvent(self, event, ...);</OnEvent>
    </Scripts>
  </Frame>
  <Frame name="MRT_GetDKPValueFrame" toplevel="true" frameStrata="DIALOG" movable="true" hidden="true" enableMouse="true" parent="UIParent">
    <Size x="555" y="150"/>
    <Anchors>
      <Anchor point="CENTER" relativePoint="CENTER" relativeTo="$parent"></Anchor>
    </Anchors>
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <TileSize val="32"/>
      <EdgeSize val="32"/>
      <BackgroundInsets left="11" right="12" top="12" bottom="11"/>
    </Backdrop>
    <Layers>
      <Layer level="ARTWORK">
        <Texture name="$parent_HeaderTexture" file="Interface\DialogFrame\UI-DialogBox-Header">
          <Size x="300" y="64"/>
          <Anchors>
            <Anchor point="TOP">
              <Offset x="0" y="12"/>
            </Anchor>
          </Anchors>
        </Texture>
      </Layer>
      <Layer level="OVERLAY">
        <FontString name="$parent_Title" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="TOP">
              <Offset x="0" y="-2"/>
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parent_Text" inherits="GameFontNormal">
          <Anchors>
            <Anchor point="TOP">
              <Offset x="0" y="-32"/>
            </Anchor>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <EditBox name="$parent_EB" historyLines="0" enableKeyboard="true" letters="100">
        <Size x="300" y="32"/>
        <Anchors>
          <Anchor point="CENTER"></Anchor>
        </Anchors>
        <Layers>
          <Layer level="BACKGROUND">
            <Texture file="Interface\ChatFrame\UI-ChatInputBorder-Left">
              <Size>
                <AbsDimension x="165" y="32"></AbsDimension>
              </Size>
              <Anchors>
                <Anchor point="LEFT">
                  <Offset>
                    <AbsDimension x="-15" y="0"></AbsDimension>
                  </Offset>
                </Anchor>
              </Anchors>
              <TexCoords left="0" right="0.65" top="0" bottom="1.0"></TexCoords>
            </Texture>
            <Texture file="Interface\ChatFrame\UI-ChatInputBorder-Right">
              <Size>
                <AbsDimension x="165" y="32"></AbsDimension>
              </Size>
              <Anchors>
                <Anchor point="RIGHT">
                  <Offset>
                    <AbsDimension x="15" y="0"></AbsDimension>
                  </Offset>
                </Anchor>
              </Anchors>
              <TexCoords left="0.35" right="1.0" top="0" bottom="1.0"></TexCoords>
            </Texture>
          </Layer>
        </Layers>
        <FontString inherits="ChatFontNormal"/>
      </EditBox>
      <Button name="$parent_OKButton" hidden="false" inherits="GameMenuButtonTemplate">
        <Size x="95" y="22"/>
        <Anchors>
          <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" relativeTo="$parent">
            <Offset x="20" y="25"/>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnClick>MRT_DKPFrame_Ok();</OnClick>
        </Scripts>
      </Button>
      <Button name="$parent_CancelButton" hidden="false" inherits="GameMenuButtonTemplate">
        <Size x="95" y="22"/>
        <Anchors>
          <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parent_OKButton">
            <Offset x="10" y="0"/>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnClick>MRT_DKPFrame_Cancel();</OnClick>
        </Scripts>
      </Button>
      <Button name="$parent_DeleteButton" hidden="false" inherits="GameMenuButtonTemplate">
        <Size x="95" y="22"/>
        <Anchors>
          <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parent_CancelButton">
            <Offset x="10" y="0"/>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnClick>MRT_DKPFrame_Delete();</OnClick>
        </Scripts>
      </Button>
      <Button name="$parent_BankButton" hidden="false" inherits="GameMenuButtonTemplate">
        <Size x="95" y="22"/>
        <Anchors>
          <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parent_DeleteButton">
            <Offset x="10" y="0"/>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnClick>MRT_DKPFrame_Bank();</OnClick>
        </Scripts>
      </Button>
      <Button name="$parent_DisenchantedButton" hidden="false" inherits="GameMenuButtonTemplate">
        <Size x="95" y="22"/>
        <Anchors>
          <Anchor point="LEFT" relativePoint="RIGHT" relativeTo="$parent_BankButton">
            <Offset x="10" y="0"/>
          </Anchor>
        </Anchors>
        <Scripts>
          <OnClick>MRT_DKPFrame_Disenchanted();</OnClick>
        </Scripts>
      </Button>
    </Frames>
    <Scripts>
      <OnLoad>self:RegisterForDrag("LeftButton");</OnLoad>
      <OnDragStart>self:StartMoving();</OnDragStart>
      <OnDragStop>self:StopMovingOrSizing();</OnDragStop>
    </Scripts>
  </Frame>
</Ui>